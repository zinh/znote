    = javascript_include_tag "//ajax.googleapis.com/ajax/libs/angularjs/1.2.14/angular-route.min.js"  
    = javascript_include_tag "//ajax.googleapis.com/ajax/libs/angularjs/1.2.14/angular-sanitize.min.js"  
    = javascript_include_tag "//code.jquery.com/jquery-1.11.0.min.js"  
    = javascript_include_tag "//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"  
    = javascript_include_tag "application"  
    = csrf_meta_tags  
  %body
    = yield
